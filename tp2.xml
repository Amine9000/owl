<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:ex="http://example.org/products#">

    <!-- Define Classes -->
    <owl:Class rdf:about="http://example.org/products#Product" />
    <owl:Class rdf:about="http://example.org/products#Category" />

    <!-- Define Properties -->
    <owl:ObjectProperty rdf:about="http://example.org/products#belongsToCategory">
        <rdfs:domain rdf:resource="http://example.org/products#Product" />
        <rdfs:range rdf:resource="http://example.org/products#Category" />
    </owl:ObjectProperty>
    <owl:DatatypeProperty rdf:about="http://example.org/products#hasName">
        <rdfs:domain rdf:resource="http://example.org/products#Product" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="http://example.org/products#hasPrice">
        <rdfs:domain rdf:resource="http://example.org/products#Product" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float" />
    </owl:DatatypeProperty>

    <!-- Define Individuals -->
    <!-- Products -->
    <ex:Product rdf:about="http://example.org/products#Product1">
        <ex:belongsToCategory rdf:resource="http://example.org/products#Category1" />
        <ex:hasName>Product One</ex:hasName>
        <ex:hasPrice rdf:datatype="http://www.w3.org/2001/XMLSchema#float">10.99</ex:hasPrice>
    </ex:Product>
    <ex:Product rdf:about="http://example.org/products#Product2">
        <ex:belongsToCategory rdf:resource="http://example.org/products#Category2" />
        <ex:hasName>Product Two</ex:hasName>
        <ex:hasPrice rdf:datatype="http://www.w3.org/2001/XMLSchema#float">20.49</ex:hasPrice>
    </ex:Product>

    <!-- Categories -->
    <ex:Category rdf:about="http://example.org/products#Category1" />
    <ex:Category rdf:about="http://example.org/products#Category2" />
</rdf:RDF>